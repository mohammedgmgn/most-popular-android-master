  },
  "description": "Slider UI component for React",
  "devDependencies": {
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.0.0",
    "jest": "^20.0.0",
    "pre-commit": "1.x",
    "rc-tools": "8.x",
    "rc-trigger": "^2.2.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
