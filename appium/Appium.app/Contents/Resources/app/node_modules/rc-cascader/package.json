{
  "author": {
    "name": "afc163@gmail.com"
  },
  "config": {
    "port": 8000
  },
  "dependencies": {
    "array-tree-filter": "^2.1.0",
    "prop-types": "^15.5.8",
    "rc-trigger": "^2.2.0",
    "rc-util": "^4.0.4",
    "react-lifecycles-compat": "^3.0.4",
    "shallow-equal": "^1.0.0",
    "warning": "^4.0.1"
  },
  "description": "cascade select ui component for react",
  "devDependencies": {
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "pre-commit": "1.x",
    "rc-form": "^2.4.0",
    "rc-tools": "^8.4.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "files": [
    "lib",
    "es",
    "assets/*.css"
  ],
  "homepage": "https://github.com/react-component/cascader",
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*"
    ],
